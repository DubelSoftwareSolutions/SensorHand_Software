#include "math.h"
#include "adc.h"
#include "dma.h"
#include "MeasurementStruct.h"
#include "Measurements.h"
#include "DataProcessing.h"

void AggregateMeasurementsToVoltage()
{
	for(int i=0; i<FLEX_SENSOR_COUNT;++i)
		SUPPLY_VOLTAGE * g_Measurements.FlexSensor[i] / pow(2,hadc2.Init.Resolution);
}
